{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "paths": {
      "@/services/*": ["../../services/*"],
      "@/types/*": ["../../types/*"]
    },
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  //"include": ["*.ts"],
  "exclude": ["node_modules", "dist"]
}
